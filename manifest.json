{
  "manifest_version": 3,
  "name": "Go Back To Your Work",
  "description": "Help you focusing on your work, blocks distracting websites",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "./src/icon.png"
  },
  "permissions": ["tabs", "activeTab", "scripting", "nativeMessaging", "notifications", "storage"],
  "background": {
    "service_worker": "./dist/background.js",
    "type": "module"
  }
}